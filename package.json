{
  "name": "random-di-workspaces",
  "version": "0.0.0",
  "description": "Random di workspace",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/botflux/random-di.git"
  },
  "scripts": {
    "test": "npm run test --ws",
    "test:watch": "npm run test:watch --ws",
    "test:coverage": "npm exec -ws -- jest --coverage --coverageReporters=text-lcov",
    "test:coverage:publish": "cat coverage/lcov.info | coveralls",
    "build": "npm run build --ws",
    "docs:dev": "vuepress dev docs",
    "docs:prod": "vuepress build docs"
  },
  "keywords": [
    "di"
  ],
  "author": "Victor Mendele <victor.mendele68@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/botflux/random-di/issues"
  },
  "homepage": "https://github.com/botflux/random-di#readme",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "coveralls": "^3.1.0",
    "vuepress": "^1.8.2",
    "vuepress-plugin-code-copy": "^1.0.6"
  }
}
